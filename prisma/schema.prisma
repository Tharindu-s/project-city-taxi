generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Guest {
  id      Int      @id @default(autoincrement())
  name    String?
  email   String?
  phone   String?
  city    String
  Users Users[]
}

 model Users {
   id         Int     @id @default(autoincrement())
   email      String  @unique
   password   String
   guest      Guest   @relation(fields: [guestId], references: [id])
   guestId    Int     @unique
 }

  model Test {
   id         Int     @id @default(autoincrement())
   data       String
 }

